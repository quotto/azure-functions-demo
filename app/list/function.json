{
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route":"list/{date}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "inDocument",
      "type": "cosmosDB",
      "databaseName": "InOut",
      "collectionName": "InOut",
      "createIfNotExists": true,
      "connectionStringSetting": "COSMOSDB_CONNECTION",
      "direction": "in",
      "sqlQuery": "SELECT * from c where c.date = {date}",
    }
  ],
  "scriptFile": "../dist/list/index.js"
}