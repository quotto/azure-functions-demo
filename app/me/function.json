{
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "todoDocument",
      "type": "cosmosDB",
      "databaseName": "ToDo",
      "collectionName": "ToDoCollection",
      "createIfNotExists": true,
      "connectionStringSetting": "COSMOSDB_CONNECTION",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/me/index.js"
}